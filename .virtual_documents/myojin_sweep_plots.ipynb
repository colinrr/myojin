foi = 'myojin_Q8_Z0900_Zw500';

max_P = 2e7 # Max overpressure
sigma = 2.0 # Gaussian filter size (pix)

# --> Sweep sets for which to make regime plots
logQ = [8., 9.]
Ztot = [1400.,2200.,6000.]

# Plot boundary contours only for explosive and intrusive PLOT codes
